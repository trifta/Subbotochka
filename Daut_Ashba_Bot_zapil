var players = 12; 
var liga =2; //лига по порядку от 1 бронза
var money =300;
setInterval(function(){
if(gam_state==""){
$("#buttonCreateGame").click();
$("#crt_players").val(players);
$("#crt_league").val(liga);
$("#crt_bet").val(money);
$("#crt_prior").click();
setTimeout(function(){
$("#gmc_btn_create").click();
},500)
}
$($('.footerButtons').find('button')[0]).click();
},1000)
